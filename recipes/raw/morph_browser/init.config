<config>
	<parent-provides>
		<service name="CPU"/>
		<service name="File_system"/>
		<service name="LOG"/>
		<service name="Gui"/>
		<service name="Nic"/>
		<service name="PD"/>
		<service name="Report"/>
		<service name="RM"/>
		<service name="ROM"/>
		<service name="Rtc"/>
		<service name="Timer"/>
		<service name="Audio_out"/>
		<service name="Audio_in"/>
		<service name="Capture"/>
		<service name="Gpu"/>
	</parent-provides>

	<default-route> <any-service> <parent/> <any-child/> </any-service> </default-route>

	<start name="jitter_sponge" caps="250">
		<resource name="RAM" quantum="1M"/>
		<provides>
			<service name="Terminal"/>
		</provides>
		<config/>
	</start>

	<start name="morph-browser" caps="4500">
		<resource name="RAM" quantum="1900M"/>
		<config clipboard="yes" ld_verbose="yes">
			<vfs>
				<dir name="dev">
					<dir name="pipe"> <pipe/> </dir>
					<log/>
					<terminal name="random"/>
					<terminal name="urandom"/>
					<rtc/>
					<oss name="dsp"/>
					<capture/>
					<gpu/>
				</dir>
				<!--<dir name=".config"> <fs label="config"/> </dir>-->
				<!--<dir name="Downloads"> <fs label="downloads"/> </dir>-->
				<dir name="shm"> <qtwebengine_shm/> </dir>
				<dir name="socket">
					<lwip dhcp="yes"/>
				</dir>
				<dir name="tmp"> <ram/> </dir>
				<tar name="qt5_dejavusans.tar"/>
				<tar name="qt5_declarative_qml.tar"/>
				<tar name="qt5_libqgenode.tar"/>
				<tar name="qt5_libqsqlite.tar"/>
				<tar name="qt5_libqsvg.tar"/>
				<tar name="qt5_graphicaleffects_qml.tar"/>
				<tar name="qt5_quickcontrols2_qml.tar"/>
				<tar name="qt5_webengine_qml.tar"/>
				<tar name="qt5_webengine_resources.tar"/>
				<tar name="qt5_webengine_translations.tar"/>
				<tar name="ubuntu-ui-toolkit_qml.tar"/>
				<tar name="ubuntu-ui-extras_qml.tar"/>
				<tar name="morph-browser_qml.tar"/>
				<tar name="ubuntu-themes.tar"/>
				<ram/>
			</vfs>
			<libc stdout="/dev/log" stderr="/dev/log" pipe="/dev/pipe" rtc="/dev/rtc" socket="/socket" pid="1">
				<mmap align_log2="21"/>
<!--
				<pthread placement="manual">
					<thread id="14" cpu="2"/>
					<thread id="23" cpu="1"/>
				</pthread>
-->
			</libc>
			<arg value="morph-browser"/>
			<arg value="--single-process"/>
			<arg value="--no-sandbox"/>
			<arg value="--disable-gpu-compositing"/>
			<arg value="--enable-logging"/>
			<arg value="--log-level=4"/>
			<arg value="--v=0"/>
			<arg value="--force-webrtc-ip-handling-policy=default_public_interface_only"/>
			<arg value="https://genode.org/files/pinephone/"/>
			<env key="QT_LOGGING_RULES" value="qml.debug=false"/>
			<env key="GRID_UNIT_PX" value="16"/>
			<!--<env key="QT_QUICK_BACKEND" value="software"/>-->
			<!--<env key="QSG_INFO" value="1"/>-->
			<!--<env key="QT_LOGGING_RULES" value="qt.scenegraph.time.renderloop=true;qt.scenegraph.time.texture=true;qt.scenegraph.time.renderer=true"/>-->
			<!--<env key="QT_LOGGING_RULES" value="qt.scenegraph.time.renderloop=true"/>-->
		</config>
		<route>
			<service name="File_system" label="config"> <parent label="config"/> </service>
			<service name="File_system" label="downloads"> <parent label="downloads"/> </service>
			<service name="Report" label="clipboard"> <parent label="clipboard"/> </service>
			<service name="ROM" label="clipboard"> <parent label="clipboard"/> </service>
			<service name="ROM" label="mesa_gpu_drv.lib.so"> <parent label="mesa_gpu_drv.lib.so" /> </service>
			<service name="Terminal"> <child name="jitter_sponge"/> </service>
			<any-service> <parent /> </any-service>
		</route>
	</start>
</config>
